						<p class="ios"></p>
						<div class="erewimabg">
							<img src="{dede:global.cfg_templets_skin/}/images/erweima.png" />				
						</div>
                    </div>
            	</div>

				 <div class="contentmiddle">
            		<div class="lefthuodong">
            			<ul>
                        	{dede:arclist typeid='1,2'  titlelen='40' row='4'  orderby='pubdate'}
