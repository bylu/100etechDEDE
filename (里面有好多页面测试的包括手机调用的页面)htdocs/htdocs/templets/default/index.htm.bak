	<script type="text/javascript" src="http://apps.bdimg.com/libs/jquery/2.1.4/jquery.min.js"></script>
	<script type="text/javascript" src="{dede:global.cfg_templets_skin/}/js/jquery.SuperSlide.2.1.1.js"></script>
	<script src="{dede:global.cfg_templets_skin/}/js/ad.js"></script>
	<script type="text/javascript">
		jQuery(".slideBox").slide({ mainCell: ".bd ul", effect: "leftLoop", vis: 3, scroll: 1, autoPlay: true });
		var tab_li = $(".linktitl ul li");
            tab_li.click(function () {
                $(this).addClass("on").siblings().removeClass("on");
                var index = tab_li.index(this);
                $("div.linkcontent  > div").eq(index).show().siblings().hide();
            });
	</script>
</html>
